{
  "NeuralNetwork": {
    "HiddenLayerNeuronsCount": 10,
    "SeriesSize": 4
  },
  "GeneticAlgorithm": {
    "PopulationSize": 500,
    "MutationProbability": 0.3,
    "MaxMutationPercentage": 0.05,
    "InsertBestIndividualGenerationInterval": 500,
    "MaxGenerations": 1000000
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "crypto-looser.log"
        }
      }
    ]
  }
}
