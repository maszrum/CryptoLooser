{
  "NeuralNetwork": {
    "HiddenLayerNeuronsCount": 10,
    "InputLengths": {
      "ClosePrice": 8,
      "PriceDifference": 8,
      "CandlestickHeight": 2,
      "Volume": 2,
      "TradesCount": 2
    }
  },
  "GeneticAlgorithm": {
    "PopulationSize": 500,
    "MutationProbability": 0.3,
    "MaxMutationPercentage": 0.05,
    "InsertBestIndividualGenerationInterval": 500,
    "MaxGenerations": 1000000
  },
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "crypto-looser.log"
        }
      }
    ]
  }
}
